{
  "root": "public/",
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/**.css": {
      "Cache-Control": "public, max-age=0, immutable"
    },
    "/**.js": {
      "Cache-Control": "public, max-age=0, immutable"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=0, immutable"
    },
    "/icons/*.png": {
      "Cache-Control": "public, max-age=0, immutable"
    }
  },
  "https_only": true,
  "error_page": "404.html"
}